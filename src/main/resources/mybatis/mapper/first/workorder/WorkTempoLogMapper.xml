<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dechuan.mapper.first.workorder.WorkTempoLogMapper">
  <resultMap id="BaseResultMap" type="com.example.dechuan.model.workorder.WorkTempoLog">
    <id column="wtky" jdbcType="INTEGER" property="wtKy" />
    <result column="woKy" jdbcType="INTEGER" property="woKy" />
    <result column="timeStatus" jdbcType="INTEGER" property="timeStatus" />
    <result column="timeStatusDesc" jdbcType="VARCHAR" property="timeStatusDesc" />
  </resultMap>
  <select id="doGetWorkOrderTimeLogList"  resultMap="BaseResultMap">
    SELECT
      wtKy,
      l.woKy,
      carNo,
      entranceDateTime,
      exitDateTime,
      timeStatus,
      timeStatusDesc
    FROM work_tempo_log l
    LEFT JOIN truckinout_workorder w on l.woKy = w.woKy
    WHERE  1=1
    <if test="timeStatus != null">
      AND timeStatus = #{timeStatus}
    </if>
      <if test="woKy != null">
          AND woKy = #{woKy}
      </if>
  </select>


  <insert id="doGetWorkOrderTimeLogAdd" parameterType="com.example.dechuan.model.workorder.WorkTempoLog">
    insert into work_tempo_log
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="woKy != null">
        woKy,
      </if>
      <if test="timeStatus != null">
        timeStatus,
      </if>
      <if test="timeStatusDesc != null">
        timeStatusDesc,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="woKy != null">
        #{woKy,jdbcType=INTEGER},
      </if>
      <if test="timeStatus != null">
        #{timeStatus,jdbcType=INTEGER},
      </if>
      <if test="timeStatusDesc != null">
        #{timeStatusDesc,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>


  <update id="doGetWorkOrderTimeLogEdit" parameterType="com.example.dechuan.model.workorder.WorkTempoLog">
    update work_tempo_log
    <set>
      <if test="woKy != null">
        woKy = #{woKy,jdbcType=INTEGER},
      </if>
      <if test="timeStatus != null">
        timeStatus = #{timeStatus,jdbcType=INTEGER},
      </if>
      <if test="timeStatusDesc != null">
        timeStatusDesc = #{timeStatusDesc,jdbcType=VARCHAR},
      </if>
    </set>
    where wtKy = #{wtKy,jdbcType=INTEGER}
  </update>
</mapper>